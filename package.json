{
  "name": "markdown-execute",
  "displayName": "Markdown Execute",
  "description": "Execute commands directly from Markdown",
  "version": "0.0.15",
  "publisher": "hanskre",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/hanskre/markdown-execute.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown",
    "onCommand:markdown-execute.executeSelection"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "markdown-execute.executeSelection",
        "title": "Markdown Execute: Execute Selection"
      }
    ],
    "keybindings": [
      {
        "command": "markdown-execute.executeSelection",
        "key": "ctrl+f1",
        "mac": "cmd+f1",
        "when": "editorTextFocus && editorLangId == markdown"
      }
    ],
    "configuration": {
      "title": "Markdown Execute",
      "properties": {
        "markdown-execute.executeInSsh": {
          "name": "Execute commands in SSH session",
          "type": "boolean",
          "default": true,
          "description": "Allow active terminal running a SSH session as target for commands"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.19.3",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "ts-type-safe": "^1.0.13"
  }
}
